      "pattern": "^http://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^https://segmentfault.com/"
    },
    {
      "pattern": "^https://console.cloud.tencent.com/cos/bucket"
    },
    {
