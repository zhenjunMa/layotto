{
  "name": "assemblyscript",
  "version": "0.1.0",
  "scripts": {
    "asbuild:server": "asc assembly/server.ts --use abort=abort_proc_exit --target server",
    "asbuild:client": "asc assembly/client.ts --use abort=abort_proc_exit --target client",
    "asbuild": "npm run asbuild:server && npm run asbuild:client"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "devDependencies": {
    "assemblyscript": "^0.20.4"
  },
  "dependencies": {
    "@nobodyiam/proxy-runtime": "^0.1.14"
  }
}
